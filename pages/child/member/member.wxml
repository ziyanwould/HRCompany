<!--pages/child/member/member.wxml-->
<view class='first'>
   <view class='f1Left'><image class='mberPerson' src='http://www.liujiarong.top/WXImg/mberPerson.png'></image></view>
   <view class='f1Right'>
     <view>广东中住七一网络科技有限公司</view>
     <view><image class='mberVIP1'src='http://www.liujiarong.top/WXImg/mberVIP1.png'></image> <text>（有效期至2018-08-08)</text></view>
   </view>
</view>

<view class='second' wx:for='{{list}}' wx:key='{{index}}'>
   <view class='title'><image class='{{item.img1}}' src='http://www.liujiarong.top/WXImg/{{item.img1}}.png'></image> <text>{{item.vip}}</text> <text>{{item.pay}}</text></view>
   <view class='xian'></view>
   <view class='list'>
       <view wx:for='{{item.lists}}' wx:key='{{index}}' wx:for-item="items">
        <image class='{{items.img}}' src='http://www.liujiarong.top/WXImg/{{items.img}}.png'></image> 
        <text>{{items.text}}</text>
       </view>
       
   </view>
   <view class='config'>立即开通</view>
</view>
