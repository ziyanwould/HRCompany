.bannerImg image{
  width:100%;
}
 .bannerImg{
  width: 100%;
  height: 360rpx;
  overflow: hidden;
} 
.seeinput{
  position: relative;
  margin:0 15rpx;
  top: -24rpx; 
}
.seeinput input{
  display: block;
  height: 48rpx;
  background: #FFFFFF;
  box-shadow: darkgrey 10rpx 10rpx 30rpx 5rpx ;/*框阴影*/
  border-radius: 10rpx;
}
.sde{
  text-align: center;
  font-size: 20rx;
}
/*兼职列表组样式*/
 .partTime{
   margin: 13rpx 20rpx;
   background: #FFFFFF;
   padding: 40rpx 30rpx;
   border-radius: 15rpx;
   display: flex;
    align-items:center;
 }
 .partTime .left image{
   width: 88rpx;
   height: 88rpx;
 }
.partTime .right .title image{
  width: 30rpx;
  height: 30rpx;
} 
.partTime .right .title{
 display: flex;
}  
.partTime .right .title text{
 font-size: 34rpx;
 color: #424242;
 overflow: hidden;
 text-overflow:ellipsis;
 white-space: nowrap;
 flex: 1;
} 
.partTime .right .name image{
  width: 24rpx;
  height: 24rpx;

} 

.partTime .right .name text{
  font-size: 26rpx;
  color: #737373;
}  
.partTime .right .message image{
  width: 24rpx;
  height: 24rpx;
}
.partTime .right .message text{
 font-size: 23rpx;
 margin-right: 12rpx;
}
.partTime .right .message{
  display: inline-flex;
  align-items:center;
}
.partTime .right,.partTime .left{
  justify-content:flex-start;
  position: relative;
}
.partTime .right{
  padding-left: 20rpx;
  flex: 1;
}
.partTime .right .other{
  display: inline-block;
  position: absolute;
  right: 0;
  top: 60rpx;
}
  
.partTime .right image{
 margin-right:12rpx; 
}
.partTime .right .other{
 display: inline-block;
 width: 140rpx;
 height: 54rpx;
 border-radius: 54rpx;
 background: #32A6E8;
 color: #FFFFFF;
 text-align: center;
 line-height: 54rpx;
}
.bottomP{
  padding-bottom: 110rpx;
}
.bottomPX{
  padding-bottom: 140rpx;
}

.partTime .right .time{
  
  position: absolute;
  right: 0;
  bottom: -36rpx;
  display: inline-flex;
  align-items:center;
}
/*文字颜色样式*/
.stats{
  color: #03a6a6;
}
.pay{
  color: #fe644f;
}
/*全职下样式修改*/
.partTime .right  .fullTime image{
   width: 23rpx;
   height: 23rpx;
}
.fullTime .education{
  color: #ffd05b;
}

.fullTime .birthday{
 color: #fe644f;
}

.fullTime .experience{
  color: #1dc5f6;
}