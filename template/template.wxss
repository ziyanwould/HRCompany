/*兼职列表组样式*/
 .partTime{
   margin: 13rpx 20rpx;
   background: #FFFFFF;
   padding: 40rpx 30rpx;
   border-radius: 15rpx;
   display: flex;
    align-items:center;
 }
 .partTime .left image{
   width: 88rpx;
   height: 88rpx;
 }
.partTime .right .title image{
  width: 30rpx;
  height: 30rpx;
} 
.partTime .right .title{
 display: flex;
}  
.partTime .right .title text{
 font-size: 34rpx;
 color: #424242;
 overflow: hidden;
 text-overflow:ellipsis;
 white-space: nowrap;
 flex: 1;
} 
.partTime .right .name image{
  width: 24rpx;
  height: 24rpx;

} 

.partTime .right .name text{
  font-size: 26rpx;
  color: #737373;
}  
.partTime .right .message image{
  width: 24rpx;
  height: 24rpx;
}
.partTime .right .message text{
 font-size: 23rpx;
 margin-right: 12rpx;
}
.partTime .right .message{
  display: inline-flex;
  align-items:center;
}
.partTime .right,.partTime .left{
  justify-content:flex-start;
  position: relative;
}
.partTime .right{
  padding-left: 20rpx;
  flex: 1;
}
.partTime .right .other{
  display: inline-block;
  position: absolute;
  right: 0;
  top: 60rpx;
}
  
.partTime .right image{
 margin-right:12rpx; 
}
.partTime .right .other{
 display: inline-block;
 width: 140rpx;
 height: 54rpx;
 border-radius: 54rpx;
 background: #32A6E8;
 color: #FFFFFF;
 text-align: center;
 line-height: 54rpx;
}
.bottomP{
  padding-bottom: 110rpx;
}
.bottomPX{
  padding-bottom: 140rpx;
}

.partTime .right .time{
  
  position: absolute;
  right: 0;
  bottom: -36rpx;
  display: inline-flex;
  align-items:center;
}
